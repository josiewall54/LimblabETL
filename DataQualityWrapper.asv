function dq = DataQualityWrapper(file, hasEmg, hasForce, hasKin)

    dq.emg, dq.force, dq.kin, dq.neural = deal('');
    
    if hasEmg
        dq.emg = DataQualityWrapperEmg(file);
    end
    
    if hasForce
        %dq.force = DataQualityWrapperForce(file)
    end
    
    if hasKin
        %dq.kin = DataQualityWrapperKin
    end
    
end